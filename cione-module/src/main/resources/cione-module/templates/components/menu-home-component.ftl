		[#assign url = damfn.getAssetLink("jcr:" + imgItemKey)!""]
	[/#if]
[/#if]--]

[#if cmsfn.children(content)?has_content]
	[#list cmsfn.children(content) as image]
		[#if image.image?has_content]
			[#assign hasPermissions = false]
			[#if image.permisos?? && image.permisos?has_content]
				[#assign field = image.permisos.field!]
				[#switch field]
					[#case "roles"]
						[#if model.hasPermissionsRoles(image.permisos.roles!)]
							[#assign hasPermissions = true]
						[/#if]
						[#break]
					[#case "campaing"]
						[#if model.hasPermissionsCampana(image.permisos.campaing!)]
							[#assign hasPermissions = true]
						[/#if]
						[#break]
					[#default]
	    				[#break]
	    		[/#switch]
	    	[#elseif model.hasPermissionsCampana(image.campanas!)]
	    		[#assign hasPermissions = true]
			[/#if]
			[#if hasPermissions]
				[#assign url = damfn.getAssetLink("jcr:" + image.image)!""]
			[/#if]
		[/#if]
	[/#list]
[/#if]
[#assign lastPosition = model.getLastPosition()]
<section class="cmp-home" style="background-image: url('${url!""}') [#if lastPosition == 0]; margin-top:0px; [/#if]">
	[#if ctx.getUser().hasRole("OPTICAPRO")] <hr style="border: 3px solid #84bd00; margin-top: 0.5rem;"> [/#if]
    <div class="continer-fluid mr-5 ml-5">
        <div class="wrapper-item">
        	
        	[#assign userERP = model.getUserFromERP()!]
        	[#assign userMagnolia = model.getUserFromERP()!]
        	[#assign countPosition = 0]
        	[#assign faltaCierre = false]
        	
        	[#-- por defecto se colocan 2 filas de 5 elementos superiores --]
        	[#assign maxElemnents = 5]
        	
        	[#switch lastPosition]
        		[#case 1][#assign maxElemnents = 1][#break]
        		[#case 2][#assign maxElemnents = 2][#break]
        		[#case 3][#assign maxElemnents = 3][#break]
        		[#case 4][#assign maxElemnents = 2][#break]
        		[#case 5][#assign maxElemnents = 3][#break]
        		[#case 6][#assign maxElemnents = 3][#break]
        		[#default]
        			[#assign resto = (lastPosition % 5)]
		            [#if (resto == 1) || (resto == 2) ]
		                [#assign maxElemnents = 4]
		            [#else]
		                [#assign maxElemnents = 5]
		            [/#if]
        			[#break]
        	[/#switch]
        	
        	[#--  --if lastPosition = 11] [#-- equilibrado para 4 elementos 
        		[#assign maxElemnents = 4]
        	[#elseif lastPosition < 7] [#-- equilibrado para 4 elementos 
        		[#assign maxElemnents = 4]
        	[#elseif lastPosition < 5] [#-- equilibrado para 6 elementos 
        		[#assign maxElemnents = 3]
        	[/#if]--]
        	
	        [#list cmsfn.children(content.menu)?sort_by("position") as link]
				[#assign hasPermissions = false]
    			[#if link.permisos?? && link.permisos?has_content]
    				[#assign field = link.permisos.field!]
    				[#switch field]
						[#case "roles"]
							[#if model.hasPermissionsRoles(link.permisos.roles!)]
								[#assign hasPermissions = true]
							[/#if]
							[#break]
						[#case "campaing"]
							[#if model.hasPermissionsCampana(link.permisos.campaing!)]
								[#assign hasPermissions = true]
							[/#if]
							[#break]
						[#default]
		    				[#break]
		    		[/#switch]
		    	[#else]
		    		[#assign hasPermissions = true]
    			[/#if]
    			[#if hasPermissions]
    				
					[#-- Link --]
		        	[#if link.internalLink?? && link.internalLink?has_content]
		        		[#assign href = ""]
			        	[#if cmsfn.contentById(link.internalLink)?? && cmsfn.contentById(link.internalLink)?has_content]
			        		[#assign href = cmsfn.link(cmsfn.contentById(link.internalLink))]
			            	[#assign target = ""]
			            	
			            [#elseif link.externalLink?? && link.externalLink?has_content]
				            	[#assign href = link.externalLink!""]
			            		[#assign target = "_blank"]
			        	[/#if]
			        	
						[#if href?has_content && href??]
							[#assign countPosition++]
							[#if countPosition == 1]
								<div class="home-wrapper-row ${cmsfn.decode(link).title} ">
								[#assign faltaCierre = true]
							[/#if]	
				            <a title="${link.tooltip!''}" class="box-item [#if ctx.getUser().hasRole("OPTICAPRO")] green[/#if]" href="${href!'#'}" target="${target}">
				            	[#-- Imaging --]
				            		[#if link.logosvg?? && link.logo?has_content]
				            			[#switch link.logosvg]
				            				[#case "svgMisDatos"]
				            					${svgMisDatos}
				            					[#break]
				            				[#case "svgPedidos"]
				            					${svgPedidos}
				            					[#break]
				            				[#case "svgCatalogos"]
				            					${svgCatalogos}
				            					[#break]
				            				[#case "svgComunicacion"]
				            					${svgComunicacion}
				            					[#break]
				            				[#case "svgMyShop"]
				            					${svgMyShop}
				            					[#break]
				            				[#case "svgRSC"]
				            					${svgRSC}
				            					[#break]
				            			[/#switch]
				            		[#else]
										[#if link.logo?? && link.logo?has_content]
											[#assign imgItemKey = link.logo]
					
											[#if imgItemKey?? && imgItemKey?has_content]
												<img src="${damfn.getAssetLink("jcr:" +imgItemKey)!''}" alt="${link.logoAlt!''}"/>
											[/#if]
										[/#if]
									[/#if]
				                <div class="title">${cmsfn.decode(link).title}</div>
				            </a>
				        [#else]
				        	
			            [/#if]
			        [#elseif link.especialLink?? && link.especialLink?has_content]
			        	[#assign field = link.especialLink.field!]
			        	[#switch field]
							[#case "enlaceUniversity"]
								[#if !ctx.getUser().hasRole("empleado_cione_cione_university")
									&& hasPermissions]
									[#assign countPosition++]
				    				[#if countPosition == 1]
										<div class="home-wrapper-row">
										[#assign faltaCierre = true]
									[/#if]
									
									[#assign href = link.especialLink.linkName]
									[#assign target = "_blank"]
									
							        [#if userERP?? && userERP?has_content]
										<a title="${link.tooltip!''}" class="box-item [#if ctx.getUser().hasRole("OPTICAPRO")] green[/#if]" href="#" onClick="updateUniversityUser('${href}')">
										
											[#-- Imaging --]
												[#if link.logo?? && link.logo?has_content]
													[#assign imgItemKey = link.logo]
							
													[#if imgItemKey?? && imgItemKey?has_content]
														<img src="${damfn.getAssetLink("jcr:" +imgItemKey)!''}" alt="${link.logoAlt!''}"/>
													[/#if]
												[/#if]
											<div class="title">${cmsfn.decode(link).title}</div>
										</a>
									[/#if]
								[/#if]
								[#break]
							
							[#case "enlaceAudio"]
								[#if !ctx.getUser().hasRole("empleado_cione_myom")
					        		&& hasPermissions] 
					        		[#assign countPosition++] 
				    				[#if countPosition == 1]
										<div class="home-wrapper-row">
										[#assign faltaCierre = true]
									[/#if]
						        	
					            	[#assign key = link.especialLink.key]
					            	[#assign href = link.especialLink.linkName]
						            [#assign target = "_blank"]
						            [#if userERP?? && userERP?has_content]
							            <a title="${link.tooltip!''}" class="box-item [#if ctx.getUser().hasRole("OPTICAPRO")] green[/#if]" href="#" onClick="updateUser('${href}', '${key}', '${userERP.numSocio!}')">
							            
							            	[#-- Imaging --]
												[#if link.logo?? && link.logo?has_content]
													[#assign imgItemKey = link.logo]
							
													[#if imgItemKey?? && imgItemKey?has_content]
														<img src="${damfn.getAssetLink("jcr:" +imgItemKey)!''}" alt="${link.logoAlt!''}"/>
													[/#if]
												[/#if]
							                <div class="title">${cmsfn.decode(link).title}</div>
							            </a>
							        [/#if]
								[/#if]
								[#break]
							[#case "enlaceForo"]
								[#if !ctx.getUser().hasRole("empleado_cione_comunidad")
					        		&& hasPermissions] 
					        		[#assign countPosition++]
				    				[#if countPosition == 1]
										<div class="home-wrapper-row">
										[#assign faltaCierre = true]
									[/#if]
					        		
					            	[#assign href = link.especialLink.linkName]
						            [#assign target = "_blank"]
						            [#if userERP?? && userERP?has_content]
							            <a title="${link.tooltip!''}" class="box-item [#if ctx.getUser().hasRole("OPTICAPRO")] green[/#if]" href="${href}/auth.php?userid=${model.getNumSocioEncrypt()!}" target="${target}">
							            
							            	[#-- Imaging --]
												[#if link.logo?? && link.logo?has_content]
													[#assign imgItemKey = link.logo]
							
													[#if imgItemKey?? && imgItemKey?has_content]
														<img src="${damfn.getAssetLink("jcr:" +imgItemKey)!''}" alt="${link.logoAlt!''}"/>
													[/#if]
												[/#if]
							                <div class="title">${cmsfn.decode(link).title}</div>
							            </a>
							        [/#if]
							    [/#if]
								[#break]
							[#case "enlaceServicios"]
								[#if !ctx.getUser().hasRole("empleado_cione_servicios")
					        		&& hasPermissions] 
					        		[#assign countPosition++]
				    				[#if countPosition == 1]
										<div class="home-wrapper-row">
										[#assign faltaCierre = true]
									[/#if]
					        		
						            [#if userERP?? && userERP?has_content]
							            <a title="${link.tooltip!''}" class="box-item [#if ctx.getUser().hasRole("OPTICAPRO")] green[/#if]" href="#" onclick="sooServicios()">
							            
							            	[#-- Imaging --]
												[#if link.logo?? && link.logo?has_content]
													[#assign imgItemKey = link.logo]
							
													[#if imgItemKey?? && imgItemKey?has_content]
														<img src="${damfn.getAssetLink("jcr:" +imgItemKey)!''}" alt="${link.logoAlt!''}"/>
													[/#if]
												[/#if]
							                <div class="title">${cmsfn.decode(link).title}</div>
							            </a>
							        [/#if]
							    [/#if]
								[#break]
							[#default]
			    				[#break]
			    		[/#switch]
			        [/#if]
			        
		            [#assign href=""]
					
					[#-- se cierra el wrapper cuando se han introducido
					     4 elementos en la fila superior y el resto
					     de elementos se colocan en la segunda fila.
					     Esto esta preparado para 4 elementos, fila superior,
					     5 elementos fila inferior --]
					[#if countPosition == maxElemnents ]
						</div>
						[#assign faltaCierre = false]
						[#assign countPosition = 0]
					[/#if]
				[/#if]
			[/#list]
			
			[#if faltaCierre]
				</div>
			[/#if]
        </div>
               
        [@cms.area name="main" /]
    </div>
    
    <div id="modalAudio" class="modal modal-anuncio" tabindex="-1" role="dialog">
	    <div class="modal-dialog" role="document">
	      <div class="modal-content" style="height: 190px;">
	        <div class="modal-body">
	        	<div id="modal-anuncio">
	        		<div id="modal-anuncio-text"></div>
	        		<div class="panelbuttonAudio" style="padding-top: 30px;">
            			<a href="javascript:closeModalAudio()" class="closemodal">${i18n['cione-module.templates.components.menu-home-component.close']}</a>
            		</div>
            	</div>
	        </div>
	      </div>
	    </div>
	</div>
</section>